{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": false,
  "semi": true,
  "arrowParens": "always",
  "printWidth": 120,
  "bracketSpacing": true,
  "singleAttributePerLine": true,
  "endOfLine": "lf",
  "trailingComma": "all",
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.svg",
      "options": {
        "parser": "html"
      }
    }
  ]
}
